# ---- Postgres ----
POSTGRES_DB=qualitycat
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change-me-postgres-password

# ---- API ----
PORT=8081
DATABASE_URL=postgresql://postgres:change-me-postgres-password@db:5432/qualitycat
JWT_SECRET=change-me-jwt-secret
NODE_ENV=production
CORS_ORIGIN=http://academy-claude.qualitycat.com.pl
RUN_SEED=0

# ---- Backend bug mode ----
BUGS=off
BUG_AUTH_WRONG_STATUS=0
BUG_PAGINATION_MIXED_BASE=0
BUG_NPLUS1_COURSES=0
BUG_BOOKINGS_PAST_ALLOWED=0
BUG_BOOKINGS_RACE=0
BUG_BOOKINGS_LEAK=0
BUG_CORS_MISCONFIG=0

# ---- Frontend ----
VITE_API_URL=/api
VITE_BUGS=off
VITE_BUG_UI_DOUBLE_SUBMIT=0
VITE_BUG_CACHE_STALE=0
VITE_BUG_TIMEZONE_SHIFT=0
VITE_BUG_ERROR_MESSAGE=0
VITE_BUG_FORM_VALIDATION=0
VITE_BUG_XSS_DESCRIPTION=0
